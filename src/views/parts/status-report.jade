div(ng-if="event.type == 'STATUS_REPORT' ")
  h1 {{eventTime.createdTime | date : 'MMMM d' }} status report

  time {{event.eventTime | timeLapse}}

  p {{event.text}}

  ul.submissions
    li(ng-repeat="image in event.images")

      .img(ng-src="image")

  button.action.wider view prototype

  button.wider share link