li.winners.completed.no-progress(
  ng-show="vm.expanded.finalDesigns"
)
  section.checkpoint
    .point

    .lead-after

    p Congratulations! You have confirmed your winners for the final design phase.

  ul.avatars
    li(ng-repeat="avatar in vm.events.finalDesigns.winnerAvatars track by $index")
      avatar(avatar-url="{{ avatar }}")